#!/bin/bash
#pin，dir绝对路径
#变量定义等号两边不能有空格
#变量使用可以{}，不能Makefile的()
############################################
PIN="/home/e/pin/pin"
LOG="Log/axiba.log"

MY_SO="/home/e/getProcessFuncCallRelation/Obj/Main.so"

PROCESS_DIR="/home/e/getProcessFuncCallRelation/Test"
PROCESS=${PROCESS_DIR}"/test"

######################################
make
##########################################
cd $PROCESS_DIR
make
####################################
$PIN -t $MY_SO -- $PROCESS
#################################
indent -linux $LOG
######################################
#/home/e/pin/pin -t /home/e/pin/source/tools/ManualExamples/obj-ia32/inscount0.so -- /home/e/pin/source/tools/ManualExamples/obj-ia32/test
